{
    "name": "xs-live-commands-client",
    "version": "1.0.0-rc.1c",
    "main": "./dist/electron/main/main.js",
    "author": "Alian713, Prelusion & MrKirby",
    "description": "A client application used for the syncing of external calls to an XS file",
    "license": "MIT",
    "scripts": {
        "dev": "tsx .electron-vite/dev-runner.ts",
        "build": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts -m prod && electron-builder -c build.json",
        "build:win32": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --win --ia32",
        "build:win64": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --win --x64",
        "build:mac": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --mac",
        "build:dir": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --dir",
        "build:clean": "cross-env BUILD_TARGET=onlyClean tsx .electron-vite/build.ts",
        "build:prune": "tsx ./prune.ts",
        "build:all": "npm-run-all --print-name --parallel build:win32 build:win64 --serial build:prune",
        "build:web": "cross-env BUILD_TARGET=web tsx .electron-vite/build.ts",
        "pack:resources": "tsx .electron-vite/hot-updater.ts",
        "dep:upgrade": "yarn upgrade-interactive --latest",
        "postinstall": "electron-builder install-app-deps && electron-rebuild"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.6.0",
        "axios": "^1.6.8",
        "electron-log": "^5.1.2",
        "electron-updater": "^6.1.8",
        "fs": "^0.0.1-security",
        "glob": "^10.3.12",
        "semver": "^7.6.0",
        "socket.io-client": "^4.7.3",
        "steamid": "^2.0.0",
        "winreg": "^1.2.5"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@types/adm-zip": "^0.5.5",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.12.2",
        "@types/semver": "^7.5.8",
        "@types/steamid": "^2.0.3",
        "@types/winreg": "^1.2.36",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/compiler-sfc": "^3.4.21",
        "adm-zip": "^0.5.10",
        "cfonts": "^3.2.0",
        "chalk": "5.3.0",
        "cross-env": "^7.0.3",
        "del": "^7.1.0",
        "dotenv": "^16.4.5",
        "electron": "^29.1.6",
        "electron-builder": "^24.13.3",
        "electron-devtools-vendor": "^3.0.0",
        "electron-rebuild": "^3.2.9",
        "extract-zip": "^2.0.1",
        "fs-extra": "^11.2.0",
        "inquirer": "^9.2.17",
        "javascript-obfuscator": "^4.1.0",
        "listr2": "^8.1.3",
        "minimist": "^1.2.8",
        "npm-run-all": "^4.1.5",
        "pinia": "^2.1.7",
        "portfinder": "^1.0.32",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-obfuscator": "^1.1.0",
        "sass": "^1.72.0",
        "tslib": "^2.6.2",
        "tsx": "^4.7.1",
        "typescript": "^5.4.3",
        "vite": "^5.2.7",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0"
    },
    "keywords": [
        "vite",
        "electron",
        "vue3",
        "rollup"
    ]
}
