    "socket.io-client": "^4.7.3",
    "steamid": "^2.0.0",
    "tslib": "^2.6.2",
    "vue": "^3.4.8",
    "vue-class-component": "^8.0.0-0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
