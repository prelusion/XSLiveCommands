    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vuex": "^4.0.0-0"
  },
