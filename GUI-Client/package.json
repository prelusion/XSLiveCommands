{
  "name": "aoe2-live-commands",
  "version": "0.0.1",
  "description": "",
  "author": "KSneijders & Prelusion",
  "scripts": {
    "build:all": "npm run build:electron && npm run build:vue",
    "build:electron": "tsc -p electron",
    "build:vue": "vue-cli-service build",
    "start": "set NODE_ENV=development&& npm run build:all && electron ./dist/electron/main.js",
    "start:electron": "set NODE_ENV=development&& electron ./dist/electron/main.js",
    "watch:electron": "nodemon --watch electron -e ts --exec \"npm run build:electron && npm run start:electron\"",
    "watch:vue": "vue-cli-service build --watch",
    "build:production": "vue-cli-service electron:build"
  },
  "main": "background.js",
  "dependencies": {
    "axios": "^0.21.2",
