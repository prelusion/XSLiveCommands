{
  "name": "xs-live-commands",
  "productName": "XS Live Commands",
  "version": "0.1.3",
  "main": "dist/electron/main/index.js",
  "description": "",
  "author": "Prelusion, Alian & KSneijders",
  "scripts": {
    "build:all": "npm run build:electron && npm run build:vue",
    "build:electron": "tsc --project .\\electron\\",
    "build:vue": "vue-cli-service build",
    "start": "set NODE_ENV=development&& npm run build:all && electron ./dist/electron/main.js",
    "start:electron": "set NODE_ENV=development&& electron ./dist/electron/main.js",
    "watch:electron": "nodemon --watch .\\electron\\ --watch .\\.env -e ts --exec \"npm run build:electron && npm run start:electron\"",
    "watch:vue": "vue-cli-service build --watch",
    "build:production": "npm run build:electron && vue-cli-service electron:build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "dependencies": {
    "native-reg": "^1.1.0",
    "socket.io-client": "^4.5.1",
    "steamid": "^2.0.0",
    "tslib": "^2.4.0",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/node": "^18.7.5",
    "@types/steamid": "^2.0.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "dotenv": "^16.0.1",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "8",
    "eslint-plugin-vue": "9.3.0",
    "nodemon": "^2.0.16",
    "sass": "^1.26.5",
    "ts-loader": "~9.3.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "vite": "^3.0.7",
    "vite-plugin-electron": "^0.9.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "webpack": "^5.74.0",
    "vue-tsc": "^0.40.1"
  }
}
