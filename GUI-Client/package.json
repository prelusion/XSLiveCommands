    "electron-toolkit": "^1.0.24",
    "electron-vite": "^2.0.0",
    "eslint": "8",
    "eslint-plugin-vue": "9.19.2",
    "nodemon": "^3.0.2",
    "sass": "^1.69.7",
    "ts-loader": "~9.5.1",
    "ts-node": "^10.9.2",
