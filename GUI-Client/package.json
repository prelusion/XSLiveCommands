    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
